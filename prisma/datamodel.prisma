type Food {
    id: ID! @id
    createdAt: DateTime! @createdAt
    name: String!
    quantity: Int!
    inCart: Boolean!
    postedBy: User
    votes: [Vote!]!
}

type User {
    id: ID! @id
    name: String!
    email: String! @unique
    password: String!
    foods: [Food!]!
    votes: [Vote!]!
}

type Vote {
    id: ID! @id
    food: Food!
    user: User!
}